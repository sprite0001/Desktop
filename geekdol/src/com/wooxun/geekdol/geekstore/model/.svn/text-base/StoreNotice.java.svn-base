package com.wooxun.geekdol.geekstore.model;

import java.util.List;

import com.soft863.dolphin.common.CommonEntity;
import com.wooxun.geekdol.system.model.Attach;

public class StoreNotice extends CommonEntity{
    
	private static final long serialVersionUID = 1L;
    /** 公告Id */
    private Long id;

    /** 店铺ID */
    private Long storeId;

    /** 公告内容 */
    private String content;

    /** 操作人 */
    private Long opreator;
    
    private List<Attach> attachs;

    public List<Attach> getAttachs() {
		return attachs;
	}

	public void setAttachs(List<Attach> attachs) {
		this.attachs = attachs;
	}

	/**
     *
     * 获取  公告Id
     *
     * @return  id
     */
    public Long getId() {
        return id;
    }

    /**
     *
     * 设置  公告Id
     *
     * @param id
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     *
     * 获取  店铺ID
     *
     * @return  storeId
     */
    public Long getStoreId() {
        return storeId;
    }

    /**
     *
     * 设置  店铺ID
     *
     * @param storeId
     */
    public void setStoreId(Long storeId) {
        this.storeId = storeId;
    }

    /**
     *
     * 获取  公告内容
     *
     * @return  content
     */
    public String getContent() {
        return content;
    }

    /**
     *
     * 设置  公告内容
     *
     * @param content
     */
    public void setContent(String content) {
        this.content = content == null ? null : content.trim();
    }

    /**
     *
     * 获取  操作人
     *
     * @return  opreator
     */
    public Long getOpreator() {
        return opreator;
    }

    /**
     *
     * 设置  操作人
     *
     * @param opreator
     */
    public void setOpreator(Long opreator) {
        this.opreator = opreator;
    }
}