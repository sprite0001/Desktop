package com.wooxun.geekdol.hmedia.model;

import com.soft863.dolphin.common.CommonEntity;

public class IntimateNewsLike extends CommonEntity {
    private static final long serialVersionUID = 1L;

    /** 主键 */
    private Long id;

    /** 贴心报id */
    private Long hearId;

    /** 点赞人的Ip */
    private String opreaterIp;

    /**
     *
     * 获取  主键
     *
     * @return  id
     */
    public Long getId() {
        return id;
    }

    /**
     *
     * 设置  主键
     *
     * @param id
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     *
     * 获取  贴心报id
     *
     * @return  hearId
     */
    public Long getHearId() {
        return hearId;
    }

    /**
     *
     * 设置  贴心报id
     *
     * @param hearId
     */
    public void setHearId(Long hearId) {
        this.hearId = hearId;
    }

    /**
     *
     * 获取  点赞人的Ip
     *
     * @return  opreaterIp
     */
    public String getOpreaterIp() {
        return opreaterIp;
    }

    /**
     *
     * 设置  点赞人的Ip
     *
     * @param opreaterIp
     */
    public void setOpreaterIp(String opreaterIp) {
        this.opreaterIp = opreaterIp == null ? null : opreaterIp.trim();
    }

}