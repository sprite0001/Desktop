public class AppActivityVo{
    
    /**id*/
    private Long id;
    
    /** 活动汇标题 */
    private String title;

    /** 报名截止时间 */
    private Date enrollEnd;
    
    /** url */
    private String url;
    
    /**报名人数*/
    private Long enrollCount;
    
    /**活动详情*/
    private String content; 
    
    /**浏览量*/
    private Long viewNumber;

    /** 更新时间 */
    private Date updYmdhms;

    /** 更新回数 */
    private Long updEac;
    
    /** 分享链接 */
    private String shareUrl;
    
    /** 是否需要报名 */
    private String enrollFlag;
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getTitle() {
        return title;
    }

    
    public void setTitle(String title) {
        this.title = title;
    }

    public String getUrl() {
        return url;
    }

    public void setUrl(String url) {
        this.url = url;
    }

    public Long getEnrollCount() {
        return enrollCount;
    }

    public String getContent() {
        return content;
    }

    public void setEnrollCount(Long enrollCount) {
        this.enrollCount = enrollCount;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Long getViewNumber() {
        return viewNumber;
    }

    public void setViewNumber(Long viewNumber) {
        this.viewNumber = viewNumber;
    }
 
    public Date getUpdYmdhms() {
        return updYmdhms;
    }

    public Long getUpdEac() {
        return updEac;
    }
 
    public void setUpdYmdhms(Date updYmdhms) {
        this.updYmdhms = updYmdhms;
    }

    public void setUpdEac(Long updEac) {
        this.updEac = updEac;
    }

    public Date getEnrollEnd() {
        return enrollEnd;
    }

    public void setEnrollEnd(Date enrollEnd) {
        this.enrollEnd = enrollEnd;
    }

	public String getShareUrl() {
		return shareUrl;
	}

	public void setShareUrl(String shareUrl) {
		this.shareUrl = shareUrl;
	}

    public String getEnrollFlag() {
        return enrollFlag;
    }

    public void setEnrollFlag(String enrollFlag) {
        this.enrollFlag = enrollFlag;
    }

    
 
}