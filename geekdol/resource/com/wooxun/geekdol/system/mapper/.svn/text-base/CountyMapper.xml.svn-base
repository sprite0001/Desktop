<!-- 根据多个县区id查找所有县区,排除删除和禁用的 -->
<select id="selectCountyByIds" resultMap="BaseResultMap" parameterType="java.util.Map" >
    select 
    <include refid="Base_Column_List" />
    from s_county
    where county_id in(${queryIds}) and del_flag = '0' and status = '0'
</select>
