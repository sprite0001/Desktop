			and b.sellModel like CONCAT('%',CONCAT(#{sellModel},'%'))
		</if>
		<if test="status !=null and sellerName.trim()!=''">
			and b.`status` = #{status,jdbcType=CHAR}
		</if>
	</where>
  </select>
  <!-- 合作店查询:订单查询 end -->
</mapper>