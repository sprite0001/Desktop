public class AppLunboVo extends BaseVo {
	/** 主键 */
	private Long id;

	/** 模块编码,模块在数据字典表中定义 */
	private String moduleCode;

	/** 开始标识：未开始，展示中，已结束 */
	private String beginFlag;

	/** 广告链接方式：图片，html，视频，网址 */
	private String linkType;

	/** 链接内容，也就是上传文件的url */
	private String linkContenturl;

	/** 广告图片url */
	private String adverPic;

	/** 广告方式:图片广告,文字广告 */
	private String adverType;

	/** 所属小区 */
	private Long villageId;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getModuleCode() {
		return moduleCode;
	}

	public void setModuleCode(String moduleCode) {
		this.moduleCode = moduleCode;
	}

	public String getBeginFlag() {
		return beginFlag;
	}

	public void setBeginFlag(String beginFlag) {
		this.beginFlag = beginFlag;
	}

	public String getLinkType() {
		return linkType;
	}

	public void setLinkType(String linkType) {
		this.linkType = linkType;
	}

	public String getLinkContenturl() {
		return linkContenturl;
	}

	public void setLinkContenturl(String linkContenturl) {
		this.linkContenturl = linkContenturl;
	}

	public String getAdverPic() {
		return adverPic;
	}

	public void setAdverPic(String adverPic) {
		this.adverPic = adverPic;
	}

	public String getAdverType() {
		return adverType;
	}

	public void setAdverType(String adverType) {
		this.adverType = adverType;
	}

	public Long getVillageId() {
		return villageId;
	}

	public void setVillageId(Long villageId) {
		this.villageId = villageId;
	}
}